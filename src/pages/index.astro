---
import Layout from "../layouts/Layout.astro";
import { getBlogs } from "../libs/microcms";
const response = await getBlogs({ fields: ["id", "title"] });
---

<Layout title="TOP">
  <h1>Bean2Cup</h1>
  <main>
    <ul>
      {
        response.contents.map((content: any) => (
          <li>
            <a href={content.id}>{content.title}</a>
          </li>
        ))
      }
    </ul>
  </main>
</Layout>
